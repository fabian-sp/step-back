{
    "dataset": "cifar10",
    "dataset_kwargs": {"as_image": false},
    "model": "vgg16",
    "loss_func": "cross_entropy",
    "score_func": "cross_entropy_accuracy",
    "opt": [{"name": "momo", "lr": [1, 1e-1, 1e-2], "weight_decay": 0, "beta":[0.9, 0.5, 0.1], "lr_schedule": "constant"},
            {"name": "sgd-m", "lr": [1e-1,1e-2], "weight_decay": 0, "lr_schedule": "constant"},
            {"name": "adam", "lr": [1e-1, 1e-2, 1e-3], "weight_decay": 0}
            ],
    "batch_size": 128,
    "max_epoch": 40,
    "n_runs": 1
    }